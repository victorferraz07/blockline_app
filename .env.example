# Arquivo de exemplo - Copie para .env e preencha com seus valores reais
# NUNCA commite o arquivo .env com dados sensíveis!

# Chave secreta do Django (gere uma nova em: https://djecrety.ir/)
SECRET_KEY=sua-secret-key-super-secreta-aqui-com-mais-de-50-caracteres

# Configurações do Banco de Dados PostgreSQL
DB_NAME=gestor_db
DB_USER=postgres
DB_PASSWORD=sua_senha_aqui
DB_HOST=localhost
DB_PORT=5432

# Configurações de Debug (use False em produção!)
DEBUG=True

# Hosts permitidos (em produção, adicione seu domínio)
ALLOWED_HOSTS=localhost,127.0.0.1
